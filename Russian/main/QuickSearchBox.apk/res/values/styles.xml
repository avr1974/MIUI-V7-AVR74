<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Suggestion">
        <item name="android:background">@android:drawable/list_selector_background</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/suggestion_view_height</item>
    </style>
    <style name="FocusingSuggestion" parent="@style/Suggestion">
        <item name="android:background">@drawable/highlighting_suggestion_background</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="CorpusName">
        <item name="android:textSize">@dimen/suggestion_group_name_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:gravity">bottom|left|center</item>
        <item name="android:paddingLeft">3.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="SearchableTipsIcon">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textColor">#7fffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/tips_icon_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">5.0dip</item>
        <item name="android:layout_weight">@integer/tips_icon_weight</item>
    </style>
    <style name="SuggestionText1">
        <item name="android:textSize">@dimen/suggestion_text1_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="RatingBar" parent="@miui:style/Widget.RatingBar.Small">
        <item name="android:gravity">center_vertical</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
    </style>
    <style name="SuggestionText2">
        <item name="android:textSize">@dimen/suggestion_text2_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:gravity">top|left|center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="SuggestionIcon1">
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">@dimen/suggestion_icon_size</item>
        <item name="android:layout_height">@dimen/suggestion_icon_size</item>
    </style>
    <style name="SuggestionIcon3">
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="WebSuggestionIcon1" parent="@style/SuggestionIcon1" />
    <style name="SuggestionIcon2">
        <item name="android:layout_width">@dimen/suggestion_icon_size</item>
        <item name="android:layout_height">@dimen/suggestion_icon_size</item>
    </style>
    <style name="SuggestionText1.Query" parent="@style/SuggestionText1" />
    <style name="SuggestionText1.Suggested" parent="@style/SuggestionText1">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="ActionButton" parent="@miui:style/Widget.Dark.Button.Rect.BlankPage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Animation.SelectSearchSource" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/corpus_selector_open</item>
        <item name="android:windowExitAnimation">@anim/corpus_selector_close</item>
    </style>
    <style name="Animation.SearchActivity" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/search_activity_in</item>
        <item name="android:windowExitAnimation">@null</item>
        <item name="android:windowShowAnimation">@null</item>
        <item name="android:windowHideAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
        <item name="android:wallpaperOpenEnterAnimation">@null</item>
        <item name="android:wallpaperOpenExitAnimation">@null</item>
        <item name="android:wallpaperCloseEnterAnimation">@null</item>
        <item name="android:wallpaperCloseExitAnimation">@null</item>
        <item name="android:wallpaperIntraOpenEnterAnimation">@null</item>
        <item name="android:wallpaperIntraOpenExitAnimation">@null</item>
        <item name="android:wallpaperIntraCloseEnterAnimation">@null</item>
        <item name="android:wallpaperIntraCloseExitAnimation">@null</item>
    </style>
    <style name="Theme.QuickSearchBox" parent="@miui:style/Theme.Dark" />
    <style name="Theme.QuickSearchBox.Search" parent="@style/Theme.QuickSearchBox">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.SearchActivity</item>
        <item name="android:windowSoftInputMode">stateUnchanged|adjustResize</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.QuickSearchBox.SearchWidgetConfig" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="Theme.SelectSearchSource" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/Animation.SelectSearchSource</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">stateUnchanged|adjustResize</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
</resources>
